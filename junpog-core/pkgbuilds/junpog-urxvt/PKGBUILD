# Maintainer: Ocillacubes <squirtle1607@rollingoak.com>
pkgname='junpog-urxvt'
pkgver=1.0
pkgrel=1
pkgdesc="Urxvt Terminal config"
arch=('x86_64')
depends=('rxvt-unicode' 'urxvt-perls')
makedepends=('git')
url="https://github.com/JunPogOS/configs"
source=('junpog-urxvt::git+https://github.com/JunPogOS/configs.git')
md5sums=('SKIP')

package() {
        mkdir -p "$pkgdir/usr/share/junpogos"
        cp "$srcdir/${pkgname}/.Xresources" "$pkgdir/usr/share/junpogos/.Xresources"
}
