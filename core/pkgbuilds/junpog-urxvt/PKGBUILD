pkgname='junpog-urxvt'
pkgver=1.0
pkgrel=1
pkgdesc="Urxvt Terminal config"
arch=('x86_64')
depends=('rxvt-unicode' 'urxvt-perls')
makedepends=('git')
url="https://github.com/Ocillacubes/junpog-dots"
source=('junpog-urxvt::git+https://github.com/Ocillacubes/junpog-dots.git')
md5sums=('SKIP')

package() {
        mkdir -p "$pkgdir/$HOME"
        cp "$srcdir/${pkgname}/.Xresources" "$pkgdir/$HOME/.Xresources"
}
